<h1>Create Account</h1>
<%= form_for @profile, controller: 'profiles', action: 'create_account' do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :city %>
<%= f.text_field :city %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :professional_industry %>
<%= f.text_field :professional_industry %>

<%= f.label :about_me %>
<%= f.text_field :about_me %>

<%= f.label :photo_url %>
<%= f.text_field :photo_url %>

<%= f.collection_select :category_name, @categories, :itself, :itself%>

<%= f.submit %>
<% end  %>

